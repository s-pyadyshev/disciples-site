mixin clans-town
    .build-container.build-container--clans
        .build__section
            span.build__section-title Other Buildings
            .row
                .build.clans__guild.build--clans
                    span Guild
                .build.clans__magic-tower.build--clans
                    span Magic Tower
                .build.clans__temple.build--clans
                    span Temple

        .build__section
            span.build__section-title Fighter Units
            .build.clans__brewery.build--clans
                span Brewery
            .build.clans__armory.build--clans
                span Armory
            .col
                .build.clans__ancestor-shrine.build--clans
                    span Ancestor Shrine
                .row
                    .build.clans__kings-hall.build--clans
                        span King's Hall
                    span.build__or OR
                    .build.clans__rune-hall.build--clans
                        span Rune Hall
            span.build__or OR
            .col
                .build.clans__outpost.build--clans
                    span Outpost
                .col
                    .build.clans__snow-den.build--clans
                        span Snow Den
                span.build__or OR
                .col
                    .build.clans__fenrirs-hall.build--clans
                        span Fenrir's Hall

        .build__section
            span.build__section-title Mage Units
            .build.clans__cottage.build--clans
                span Cottage
            .col
                .build.clans__wisdom-garden.build--clans
                    span Wisdom Garden
                .build.clans__elder-tree.build--clans
                    span Elder Tree
            span.build__or OR
            .col
                .build.clans__alchemy-tower.build--clans
                    span Alchemy Tower
            .build.clans__mountain-lair.build--clans
                span Mountain Lair

        .build__section
            span.build__section-title Ranged Attack Units
            .build.clans__shooting-range.build--clans
                span Shooting Range
            .row
                .build.clans__eternal-forge.build--clans
                    span Eternal Forge
                span.build__or OR
                .build.clans__engineers-guild.build--clans
                    span Engineer's Guild

        .build__section
            span.build__section-title Support Units
            .build.clans__mountain-peak.build--clans
                span Mountain Peak
            .col
                .build.clans__cloud-keep.build--clans
                    span Cloud Keep
                .build.clans__bifrost-bridge.build--clans
                    span Bifrost Bridge
            span.build__or OR
            .col
                .build.clans__ice-mountains.build--clans
                    span Ice Mountains
                .build.clans__ice-caves.build--clans
                    span Ice Caves

        .build__section
            span.build__section-title Background
            .row
                .build.clans__windmill.build--clans
                    span Windmill

        .btn.btn--save.btn--save-clans Save Picture

    .clans
